{
  "name": "node-client-settings",
  "version": "1.0.0",
  "description": "Client settings service",
  "main": "server.js",
  "scripts": {
    "start": "DEBUG=express:router,monk:*,client-settings:* node server.js",
    "watch": "nodemon server.js",
    "debug": "node --debug-brk server.js",
    "test": "mocha --recursive",
    "test-debug": "mocha --recursive --debug-brk",
    "test-watch": "mocha --reporter mocha-multi --reporter-options mocha-osx-reporter=-,spec=- --recursive --watch"
  },
  "author": "Johan Andersson",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "debug": "^2.5.1",
    "express": "^4.14.0",
    "monk": "^3.1.3"
  },
  "engines": {
    "node": "5.7.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.2.0",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "monk": "^3.1.3",
    "supertest": "^2.0.1"
  }
